        <group name="General" systemName="General" collapsibleState="None">
          <fields>
            <field systemName="GeneralLogo" />
          </fields>
        </group>
        <group name="Layout" systemName="Layout" collapsibleState="None">
          <fields>
            <field systemName="LayoutMode" />
            <field systemName="LayoutStyle" />
          </fields>
        </group>
        <group name="Background" systemName="Background" collapsibleState="None">
          <fields>
            <field systemName="BackgroundImage" />
            <field systemName="BackgroundCustomImage" />
            <field systemName="BackgroundColor" />
          </fields>
        </group>
        <group name="Navigation" systemName="Navigation" collapsibleState="None">
          <fields>
            <field systemName="NavigationSticky" />
            <field systemName="NavigationPosition" />
            <field systemName="NavigationStyle" />
          </fields>
        </group>
        <group name="Footer" systemName="Footer" collapsibleState="None">
          <fields>
            <field systemName="FooterCompanyName" />
            <field systemName="FooterAddress" />
            <field systemName="FooterPhone" />
            <field systemName="FooterEmail" />
          </fields>
        </group>
      </groups>
    </layout>
