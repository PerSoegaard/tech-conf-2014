<?xml version="1.0" encoding="utf-16" standalone="yes"?>
<items>
  <item category="Settings" name="Dwsimple website settings" systemName="DwsimpleWebsiteSettings" description="" icon="/Admin/Images/Ribbon/Icons/Small/preferences.png" largeIcon="/Admin/Images/Ribbon/Icons/preferences.png" includeInUrlIndex="False">
