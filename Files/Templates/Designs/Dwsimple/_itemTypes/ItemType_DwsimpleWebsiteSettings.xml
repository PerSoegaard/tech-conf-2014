            <option name="Dark" value="body-bg-1" icon="Templates/Designs/Dwsimple/images/settings/body-bg-1.png" />
            <option name="Magnetite" value="body-bg-2" icon="Templates/Designs/Dwsimple/images/settings/body-bg-2.png" />
            <option name="Grey" value="body-bg-3" icon="Templates/Designs/Dwsimple/images/settings/body-bg-3.png" />
            <option name="Light grey" value="body-bg-4" icon="Templates/Designs/Dwsimple/images/settings/body-bg-4.png" />
            <option name="Building" value="body-bg-5" icon="Templates/Designs/Dwsimple/images/settings/body-bg-5.jpg" />
            <option name="Bridge" value="body-bg-6" icon="Templates/Designs/Dwsimple/images/settings/body-bg-6.jpg" />
            <option name="None" value="None" icon="" />
          </Static>
        </options>
      </field>
      <field name="Custom image" systemName="BackgroundCustomImage" description="" type="System.String, mscorlib">
        <editor type="Dynamicweb.Content.Items.Editors.FileEditor, Dynamicweb">
          <editorConfuguration><Parameters><Parameter addin="Dynamicweb.Content.Items.Editors.FileEditor" name="Base directory" value="/Billeder" /><Parameter addin="Dynamicweb.Content.Items.Editors.FileEditor" name="Extensions" value="" /></Parameters></editorConfuguration>
        </editor>
      </field>
